{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#CAD3F5",
          "template": " ‚ùØ"
        },
        {
          "type": "root",
          "style": "diamond",
          "template": " \uf0e7 ",
          "properties": {
            "root_icon": "\uf292 "
          }
        }
      ]
    },
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#CAD3F5",
          "background": "#24273A",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4 ",
          "template": "{{ .FormattedMs }}",
          "properties": {
            "style": "round",
            "threshold": 250
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#CAD3F5",
          "background": "#24273A",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue5ff {{ .Path }}",
          "properties": {
            "style": "folder"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#CAD3F5",
          "background": "#24273A",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .HEAD }}",
          "properties": {
            "branch_icon": ""
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "background": "#24273A",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "foreground": "#7FD5EA",
          "background": "#24273A",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ufcd1{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "foreground": "#42E66C",
          "background": "#303651",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#303651",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "foreground": "#E64747",
          "background": "#303651",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "properties": {
            "fetch_version": false
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "foreground": "#9B6BDF",
          "background": "#303651",
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "properties": {
            "fetch_version": false
          }
        }
      ]
    }
  ]
}
